<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.project_math_game"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.project_math_game.MathGameActivity"
            android:configChanges="keyboardHidden|orientation"
            android:label="@string/app_name"
            android:noHistory="true"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".StartGame"
            android:label="@string/start_game_activity_title"
            android:noHistory="true" >
        </activity>
        <activity
            android:name=".Help"
            android:label="@string/help_activity_title"
            android:noHistory="true" >
        </activity>
        <activity
            android:name=".Options"
            android:label="@string/options_activity_title"
            android:noHistory="true" >
        </activity>
        <activity
            android:name=".HighScore"
            android:label="@string/high_score_activity_title"
            android:noHistory="true" >
        </activity>
        <activity
            android:name=".ResultsOfGame"
            android:label="@string/results_activity_title"
            android:noHistory="true" >
        </activity>
    </application>

</manifest>